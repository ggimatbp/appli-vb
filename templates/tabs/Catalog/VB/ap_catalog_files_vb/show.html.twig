{% extends 'base.html.twig' %}

{% block title %}ApCatalogFilesVb{% endblock %}

{% block body %}
    <h1>ApCatalogFilesVb</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ap_catalog_files_vb.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ ap_catalog_files_vb.name }}</td>
            </tr>
            <tr>
                <th>Archive</th>
                <td>{{ ap_catalog_files_vb.archive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>File_name</th>
                <td>{{ ap_catalog_files_vb.fileName }}</td>
            </tr>
            <tr>
                <th>File_size</th>
                <td>{{ ap_catalog_files_vb.fileSize }}</td>
            </tr>
            <tr>
                <th>File_type</th>
                <td>{{ ap_catalog_files_vb.fileType }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ ap_catalog_files_vb.createdAt ? ap_catalog_files_vb.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('ap_catalog_files_vb_index') }}">back to list</a>

    <a href="{{ path('ap_catalog_files_vb_edit', {'id': ap_catalog_files_vb.id}) }}">edit</a>

    {{ include('tabs/Catalog/VB/ap_catalog_files_vb/_delete_form.html.twig') }}
{% endblock %}
