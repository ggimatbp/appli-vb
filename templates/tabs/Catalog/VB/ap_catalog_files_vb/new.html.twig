{% extends 'base.html.twig' %}

{% block title %}Nouveau document{% endblock %}
{% block javascripts %}
{{ encore_entry_script_tags('apCatalogImagePreview') }}
{% endblock %}
{% block body %}

    {% for ap_access in app.user.RoleId.apAccesses %}
    	{% if ap_access.tab.name is same as(tabName) and ap_access.add is same as(true) %}
            <div class="my-5 w-100 d-flex justify-content-around">
                <a class="btn" href="{{ path('ap_sector_vb_show', {'id': sector_id}) }}">Retour</a>
                <div></div>
            </div>
            <span class="form-container-with-title">
                <h1 class="catalog-h1 mt-2">Ajout d'un nouveau document pour Velobatterie</h1>
                 {{ include('tabs/Catalog/VB/ap_catalog_files_vb/_form_new.html.twig') }} 
            </span>
        {% endif %}
    {% endfor %}
{% endblock %}