{% extends 'base.html.twig' %}

{% block title %}ApTab index{% endblock %}

{% block body %}
    <h1>ApTab index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ap_tab in ap_tabs %}
            <tr>
                <td>{{ ap_tab.id }}</td>
                <td>{{ ap_tab.name }}</td>
                <td>
                    <a href="{{ path('ap_tab_show', {'id': ap_tab.id}) }}">show</a>
                    <a href="{{ path('ap_tab_edit', {'id': ap_tab.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('ap_tab_new') }}">Create new</a>
{% endblock %}
