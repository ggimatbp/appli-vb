{% block tabcontent %}
{{ dump(users) }}
<h1>Liste des employés</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Prénom</th>
                <th>Nom</th>
                <th>Email</th>
                <th>Mot de passe</th>
                <th>Notification</th>
                <th>Active</th>
                <th>Checkin</th>
                <th>Hour_count</th>
                <th>Weekly_hour</th>
                <th>role</th>
                <th>actions</th>
                
            </tr>
        </thead>
        <tbody>
        {% for ap_employee in ap_employees %}
            <tr>
                <td>{{ ap_employee.id }}</td>
                <td>{{ ap_employee.firstname }}</td>
                <td>{{ ap_employee.lastname }}</td>
                <td>{{ ap_employee.email }}</td>
                <td>{{ ap_employee.password }}</td>
                <td>{{ ap_employee.notification ? 'Yes' : 'No' }}</td>
                <td>{{ ap_employee.active ? 'Yes' : 'No' }}</td>
                <td>{{ ap_employee.checkin ? 'Yes' : 'No' }}</td>
                <td>{{ ap_employee.hourCount }}</td>
                <td>{{ ap_employee.weeklyHour }}</td>
                <td>{{ ap_employee.Role.name }}</td>
                <td>
                    <a href="{{ path('ap_employee_show', {'id': ap_employee.id}) }}">show</a>
                    <a href="{{ path('ap_employee_edit', {'id': ap_employee.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="11">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('ap_employee_new') }}">Create new</a>

    <h1>ApTab index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>path</th>
                <th>clef de la table supérieur</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ap_tab in ap_tabs %}
            <tr>
                <td>{{ ap_tab.id }}</td>
                <td>{{ ap_tab.name }}</td>
                <td>{{ ap_tab.path }}</td>
                <td> 
                {% if ap_tab.apTab.id is defined %}
                {{ap_tab.apTab.id }}
                {% endif %}

                </td> 
                <td>
                    <a href="{{ path('ap_tab_show', {'id': ap_tab.id}) }}">show</a>
                    <a href="{{ path('ap_tab_edit', {'id': ap_tab.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('ap_tab_new') }}">Create new</a>

    {% endblock %}